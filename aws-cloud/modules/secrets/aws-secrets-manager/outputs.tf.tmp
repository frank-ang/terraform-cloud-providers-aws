output "secret_prefix" {
  value = var.secret_prefix
}

output "tm_iam_prefix" {
  value = var.tm_iam_prefix
}

output "role_permissions_boundary_arn" {
  value = aws_iam_policy.aws_sm_vault_installer_permission_boundary.arn
}

output "vault_installer_namespace" {
  value = var.vault_installer_namespace
}

output "vault_installer_serviceaccount" {
  value = var.vault_installer_serviceaccount
}

output "hault_k8s_auth_backend_name" {
  value = var.hault_kubernetes_auth_backend.name
}

output "hault_k8s_auth_backend_role" {
  value = var.hault_kubernetes_auth_backend.vault_installer_role_name
}

output "basic_auth_credentials_password" {
  value = var.basic_auth_credentials.basic_auth_password
}

output "vault_installer_role_arn" {
  value = module.aws_sm_vault_installer.iam_role_arn
}